{
	"name": "mobile",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"start": "react-native start",
		"test": "jest",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"xcode": "open -a Xcode ./ios/mobile.xcworkspace",
		"pods": "cd ios && arch -x86_64 pod install && cd ../",
		"icons": "cd ./src/parts/icons/ && node buildscript.js",
		"android:clean": "cd ./android && ./gradlew clean",
		"android:run": "npx react-native run-android"
	},
	"dependencies": {
		"@babel/plugin-proposal-export-namespace-from": "7.16.7",
		"@pulsejs/core": "4.5.4",
		"@pulsejs/react": "4.5.4",
		"@react-native-async-storage/async-storage": "1.15.16",
		"@react-native-clipboard/clipboard": "^1.8.5",
		"@react-native-community/push-notification-ios": "^1.10.1",
		"@react-native-menu/menu": "^0.5.2",
		"@react-navigation/bottom-tabs": "6.0.0",
		"@react-navigation/native": "6.0.6",
		"@react-navigation/native-stack": "6.2.5",
		"axios": "^0.24.0",
		"expo": ">=44.0.0-0 <45.0.0",
		"expo-blur": "^11.0.0",
		"expo-linear-gradient": "^11.0.3",
		"expo-media-library": "^14.0.1",
		"react": "17.0.2",
		"react-native": "0.67.2",
		"react-native-device-info": "8.4.8",
		"react-native-fast-image": "8.5.11",
		"react-native-gesture-handler": "^2.2.0",
		"react-native-image-crop-picker": "^0.37.2",
		"react-native-image-picker": "^4.7.3",
		"react-native-linear-gradient": "2.5.6",
		"react-native-popup-menu": "^0.15.12",
		"react-native-reanimated": "^2.4.1",
		"react-native-safe-area-context": "3.3.2",
		"react-native-screens": "3.10.2",
		"react-native-svg": "12.1.1",
		"react-native-vision-camera": "2.12.0",
		"styled-components": "5.3.3"
	},
	"devDependencies": {
		"@babel/core": "7.16.12",
		"@babel/runtime": "7.16.7",
		"@react-native-community/eslint-config": "2.0.0",
		"@types/axios": "0.14.0",
		"@types/jest": "26.0.24",
		"@types/react-native": "0.66.15",
		"@types/react-test-renderer": "17.0.1",
		"@types/styled-components-react-native": "5.1.3",
		"@typescript-eslint/eslint-plugin": "5.10.0",
		"@typescript-eslint/parser": "5.10.0",
		"babel-jest": "26.6.3",
		"babel-plugin-module-resolver": "4.1.0",
		"babel-plugin-transform-remove-console": "6.9.4",
		"eslint": "7.32.0",
		"jest": "26.6.3",
		"metro-react-native-babel-preset": "0.66.2",
		"react-test-renderer": "17.0.2",
		"typescript": "4.5.5"
	},
	"resolutions": {
		"@types/react": "^17",
		"styled-components": "^5",
		"react-devtools-core": "4.13.0"
	},
	"jest": {
		"preset": "react-native",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"expo": {
		"autolinking": {
			"exclude": [
				"expo-font",
				"expo-keep-awake",
				"expo-constants",
				"expo-application"
			]
		}
	}
}
