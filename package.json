{
	"name": "mobile3",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"android:build": "",
		"ios": "react-native run-ios",
		"ios:build": "react-native bundle --entry-file index.ios.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
		"start": "react-native start",
		"test": "jest",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"xcode": "open -a Xcode ./ios/mobile3.xcworkspace",
		"pods": "cd ios && arch -x86_64 pod install && cd ../",
		"icons": "node ./scripts/buildIcons.js",
		"prepare": "husky install"
	},
	"dependencies": {
		"@gorhom/bottom-sheet": "^4",
		"@notifee/react-native": "^5.6.0",
		"@react-native-clipboard/clipboard": "^1.11.0",
		"@react-native-firebase/app": "^15.4.0",
		"@react-native-firebase/messaging": "^15.4.0",
		"@react-navigation/native": "^6.0.12",
		"@react-navigation/native-stack": "^6.8.0",
		"@react-navigation/stack": "^6.2.3",
		"@shopify/flash-list": "^1.2.1",
		"axios": "^0.27.2",
		"dayjs": "^1.11.5",
		"expo": "^46.0.0",
		"expo-blur": "^11.2.0",
		"expo-media-library": "^14.2.0",
		"react": "18.0.0",
		"react-native": "0.69.4",
		"react-native-blur": "^3.2.2",
		"react-native-date-picker": "^4.2.5",
		"react-native-device-info": "^10.0.2",
		"react-native-fast-image": "^8.5.11",
		"react-native-fs": "^2.20.0",
		"react-native-gesture-handler": "^2.5.0",
		"react-native-image-picker": "^4.10.0",
		"react-native-linear-gradient": "^2.6.2",
		"react-native-mmkv-storage": "^0.8.0",
		"react-native-popup-menu": "^0.15.13",
		"react-native-reanimated": "^2.10.0",
		"react-native-safe-area-context": "^4.3.3",
		"react-native-screens": "^3.16.0",
		"react-native-splash-screen": "^3.3.0",
		"react-native-svg": "^13.0.0",
		"react-native-vision-camera": "^2.14.1",
		"simple-core-state": "^0.0.8",
		"styled-components": "^5.3.5"
	},
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@babel/plugin-proposal-export-namespace-from": "^7.18.9",
		"@babel/runtime": "^7.12.5",
		"@react-native-community/eslint-config": "^2.0.0",
		"@tsconfig/react-native": "^2.0.2",
		"@types/jest": "^26.0.23",
		"@types/node": "^18.7.13",
		"@types/react-native": "^0.69.3",
		"@types/react-test-renderer": "^18.0.0",
		"@types/styled-components": "^5.1.26",
		"@types/styled-components-react-native": "^5.1.3",
		"@typescript-eslint/eslint-plugin": "^5.29.0",
		"@typescript-eslint/parser": "^5.29.0",
		"babel-jest": "^26.6.3",
		"babel-plugin-module-resolver": "^4.1.0",
		"babel-plugin-transform-remove-console": "^6.9.4",
		"eslint": "^7.32.0",
		"husky": "^8.0.0",
		"jest": "^26.6.3",
		"lint-staged": "^13.0.3",
		"metro-react-native-babel-preset": "^0.70.3",
		"react-test-renderer": "18.0.0",
		"typescript": "^4.4.4"
	},
	"jest": {
		"preset": "react-native",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"resolutions": {
		"styled-components": "5"
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"prettier --write"
		]
	}
}
